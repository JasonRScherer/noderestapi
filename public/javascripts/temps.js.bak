var chart;
var tempsFound = [];
$(document).ready(function(){
    chart = new Highcharts.Chart({
        chart:{
            renderTo: 'chart',
            type:'line',
            showAxes: true
        },
        title: {
            text:'Temperatures'
        },
        xAxis: {
            title:{
                text: 'Days'
            },
            categories:[]
        },
        yAxis: {
            title:{
                text: 'Temperatures ( C)'
            }
        },
        series: [{
            data: []
        }]
    });
    $.getJSON('/api/temperatures', function(data){
        tempsFound.length = data.length;

        for (var i=0; i <tempsFound.length; i++){
            tempsFound[i] = data[i].temp_l;
        }
    chart.series[0].setData(tempsFound);
       var start = +new Date();

    });

            });
